includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    checkMissingIterableValueType: true

    level: max

    ignoreErrors:
        -
            message: '#Property App\\Client\\IsbnClient\:\:\$[a-zA-Z]* is never read, only written.#'
            path: app/Client/IsbnClient.php

    paths:
        - app
        - bootstrap
        - database/factories
        - routes
        - tests
